SELECT TOP (1000) [ID]
      ,[PortfolioID]
      ,[PortfolioName]
      ,[MorningstarPortfolioID]
      ,[Attribute]
      ,[Ticker]
      ,[Value]
  FROM [InvestmentDB].[dbo].[MorningstarYchartsData]